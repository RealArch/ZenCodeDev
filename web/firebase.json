{
  "hosting": {
    "redirects": [
      {
        "source": "/r/:code",
        "destination": "/es/r/:code",
        "type": 301
      }
    ]
  },
  "apphosting": {
    "backendId": "zencode-server",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  }
}
